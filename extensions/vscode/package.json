{
  "name": "cuedeck",
  "displayName": "CueDeck",
  "description": "AI-native knowledge graph for markdown workspaces",
  "version": "0.1.0",
  "publisher": "cuedeck",
  "repository": {
    "type": "git",
    "url": "https://github.com/cuedeck/cuedeck"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.cuedeck/config.toml",
    "onCommand:cuedeck.search",
    "onCommand:cuedeck.graphView",
    "onView:cuedeckTasks",
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cuedeck.search",
        "title": "CueDeck: Search Documents"
      },
      {
        "command": "cuedeck.graphView",
        "title": "CueDeck: Open Graph View"
      },
      {
        "command": "cuedeck.refreshTasks",
        "title": "CueDeck: Refresh Tasks",
        "icon": "$(refresh)"
      },
      {
        "command": "cuedeck.createTask",
        "title": "CueDeck: Create Task",
        "icon": "$(add)"
      },
      {
        "command": "cuedeck.searchTasks",
        "title": "CueDeck: Search Tasks",
        "icon": "$(search)"
      },
      {
        "command": "cuedeck.openTask",
        "title": "Open Task File"
      },
      {
        "command": "cuedeck.changeTaskStatus",
        "title": "Change Status"
      },
      {
        "command": "cuedeck.changeTaskPriority",
        "title": "Change Priority"
      },
      {
        "command": "cuedeck.changeTaskAssignee",
        "title": "Change Assignee"
      },
      {
        "command": "cuedeck.createTaskFromTemplate",
        "title": "CueDeck: Create Task from Template",
        "icon": "$(file-add)"
      },
      {
        "command": "cuedeck.filterTasks",
        "title": "CueDeck: Filter Tasks",
        "icon": "$(filter)"
      },
      {
        "command": "cuedeck.sortTasks",
        "title": "CueDeck: Sort Tasks",
        "icon": "$(arrow-both)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cuedeck",
          "title": "CueDeck",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "cuedeck": [
        {
          "id": "cuedeckTasks",
          "name": "Tasks"
        },
        {
          "id": "cuedeckStats",
          "name": "Statistics"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "cuedeck.refreshTasks",
          "when": "view == cuedeckTasks",
          "group": "navigation"
        },
        {
          "command": "cuedeck.createTask",
          "when": "view == cuedeckTasks",
          "group": "navigation"
        },
        {
          "command": "cuedeck.searchTasks",
          "when": "view == cuedeckTasks",
          "group": "navigation"
        },
        {
          "command": "cuedeck.filterTasks",
          "when": "view == cuedeckTasks",
          "group": "navigation"
        },
        {
          "command": "cuedeck.sortTasks",
          "when": "view == cuedeckTasks",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "cuedeck.openTask",
          "when": "view == cuedeckTasks && viewItem == task",
          "group": "1_actions@1"
        },
        {
          "command": "cuedeck.changeTaskStatus",
          "when": "view == cuedeckTasks && viewItem == task",
          "group": "2_metadata@1"
        },
        {
          "command": "cuedeck.changeTaskPriority",
          "when": "view == cuedeckTasks && viewItem == task",
          "group": "2_metadata@2"
        },
        {
          "command": "cuedeck.changeTaskAssignee",
          "when": "view == cuedeckTasks && viewItem == task",
          "group": "2_metadata@3"
        }
      ]
    },
    "configuration": {
      "title": "CueDeck",
      "properties": {
        "cuedeck.cliPath": {
          "type": "string",
          "default": "cue",
          "description": "Path to CueDeck CLI binary"
        },
        "cuedeck.enableSync": {
          "type": "boolean",
          "default": false,
          "description": "Enable real-time sync with team members"
        }
      }
    },
    "keybindings": [
      {
        "command": "cuedeck.createTask",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "!terminalFocus"
      },
      {
        "command": "cuedeck.search",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f"
      },
      {
        "command": "cuedeck.graphView",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      },
      {
        "command": "cuedeck.refreshTasks",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "view == cuedeckTasks"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@vscode/test-electron": "^2.3.8",
    "cytoscape": "^3.33.1",
    "typescript": "^5.3.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.1"
  }
}